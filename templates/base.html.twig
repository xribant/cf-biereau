<!DOCTYPE html>
<html lang="zxx">
<head>
    <title>{% block title %}{% endblock %} | Comité des fêtes du collège du Biéreau</title>
    <meta charset="UTF-8">
    <meta name="description" content="Cassi Photo Studio HTML Template">
    <meta name="keywords" content="photo, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Stylesheets -->
    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/font-awesome.min.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/themify-icons.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/accordion.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/fresco.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/elegant-icons.css') }}"/>
    <link rel="stylesheet" href="{{ asset('css/owl.carousel.min.css') }}"/>

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

    </head>
    <body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>
    {{ flasher_render() }}
    <!-- Offcanvas Menu Section -->
    <div class="offcanvas-menu-wrapper">
        <div class="menu-header">
            <a href="{{ path('app_home') }}" class="site-logo">
                <img src="{{ asset('img/logo-cf-white.png') }}" alt="logo CF">
            </a>
            <div class="menu-switch" id="menu-canvas-close">
                <i class="icon_close"></i>
            </div>
        </div>
        <ul class="main-menu">
            <li><a href="{{ path('app_home') }}" {% if active_menu == 'home' %}class="active"{% endif %}>Accueil</a></li>
            <li><a href="#">Le comité</a></li>
            <li><a href="{{ path('app_front_gallery') }}" {% if active_menu == 'gallery' %}class="active"{% endif %}>Photos</a></li>
            <li><a href="{{ path('app_contact') }}" {% if active_menu == 'contact' %}class="active"{% endif %}>Contact</a></li>
        </ul>
        <div class="menu-footer">
            <div class="footer-social">
                <a href="https://www.mywebcreation.be" target="_blank">My Web Creation</a>
            </div>
            <div class="copyright"><p><!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                    Copyright &copy; <script>document.write(new Date().getFullYear());</script> Tous droits réservés | Comité des fêtes du collège du Biéreau
                    <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. --></p></div>
        </div>
    </div>
    <!-- Offcanvas Menu Section end -->

    {% block header %}
    <!-- Header section -->
    <header class="header-section">
        <a href="{{ path('app_home') }}" class="site-logo">
            <img src="{{ asset('img/logo-cf-white.png') }}" alt="logo CF">
        </a>
        <div class="menu-switch" id="menu-canvas-show">
            <i class="icon_menu"></i>
        </div>
    </header>
    <!-- Header section end -->
    {% endblock %}

    {% block content %}
    {% endblock %}

    <!-- Footer section -->
    <footer class="footer-section">
        <div class="footer-social">
           <a href="https://www.mywebcreation.be" target="_blank">My Web Creation</a>
        </div>
        <div class="copyright"><p><!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                Copyright &copy; <script>document.write(new Date().getFullYear());</script> Tous droits réservés | Comité des fêtes du collège du Biéreau de Louvain-La-Neuve
                <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. --></p></div>
    </footer>
    <!-- Footer section end -->

    <!--====== Javascripts & Jquery ======-->
    <script src="{{ asset('js/vendor/jquery-3.2.1.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/owl.carousel.min.js') }}"></script>
    <script src="{{ asset('js/imagesloaded.pkgd.min.js') }}"></script>
    <script src="{{ asset('js/isotope.pkgd.min.js') }}"></script>
    <script src="{{ asset('js/jquery.nicescroll.min.js') }}"></script>
    <script src="{{ asset('js/circle-progress.min.js') }}"></script>
    <script src="{{ asset('js/pana-accordion.js') }}"></script>
    <script src="{{ asset('js/fresco.min.js') }}"></script>
    <script src="{{ asset('js/masonry.pkgd.min.js') }}"></script>

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}

    </body>
</html>
